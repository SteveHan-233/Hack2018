 {\n              type: \"time\",\n              time: {\n                format: \"YYYY-MM-DD\"\n              },\n              distribution: \"linear\"\n            }\n          ]\n        },\n        maintainAspectRatio: false\n      }\n    };\n  },\n  // firebase: {\n  //   list: transactions\n  // },\n  mounted() {\n    this.user = this.$store.username;\n  },\n  computed: {\n    datacollection: function() {\n      // console.log(this.$store.username)\n      // if (!this.$store.username) {\n      //   return null;\n      // } else {\n        return {\n          // labels: this.getDaysOfMonth,\n          datasets: [\n            {\n              label: \"Earnings\",\n              data: this.earnings,\n              fill: false,\n              borderColor: \"#53f442\"\n              // backgroundColor: \"#53f442\"\n            },\n            {\n              label: \"Spendings\",\n              data: this.spendings,\n              fill: false,\n              borderColor: \"#e82929\"\n              // backgroundColor: \"#e82929\"\n            },\n            {\n              label: \"Balance\",\n              data: this.balance,\n              fill: false,\n              borderColor: \"#000000\"\n              // backgroundColor: \"#e82929\"\n            }\n          ]\n        };\n      // }\n    },\n    earnings: function() {\n      let earnings = [];\n      for (let i = 0; i < this.sorted.length; i++) {\n        if (this.sorted[i].amount > 0) {\n          console.log(this.sorted[i].transaction_date)\n          earnings.push({\n            x: this.sorted[i].transaction_date,\n            y: this.sorted[i].amount\n          });\n        }\n      }\n      earnings.sort((a, b) => new Date(a.transaction_date).getTime() - new Date(b.transaction_date).getTime());\n      return earnings;\n    },\n    spendings: function() {\n      let spendings = [];\n      for (let i = 0; i < this.sorted.length; i++) {\n        console.log(this.sorted[i].amount)\n        if (this.sorted[i].amount < 0) {\n          spendings.push({\n            x: this.sorted[i].transaction_date,\n            y: Math.abs(this.sorted[i].amount)\n          });\n        }\n      }\n      spendings.sort((a, b) => new Date(a.transaction_date).getTime() - new Date(b.transaction_date).getTime());\n      console.log(\"spendings: \" + spendings)\n      return spendings;\n    },\n    balance: function() {\n      let balance = [];\n      let sum = 0;\n      for (let i = 0; i < this.sorted.length; i++) {\n        sum += parseFloat(this.sorted[i].amount);\n        balance.push({\n          x: this.sorted[i].transaction_date,\n          y: sum\n        });\n      }\n      balance.sort((a, b) => new Date(a.transaction_date).getTime() - new Date(b.transaction_date).getTime());\n      return balance;\n    }\n  }\n};\n</script>\n\n<style scoped>\n#main {\n  align-content: center;\n  align-items: center;\n  padding: 0;\n  background-color: none;\n}\nh2 {\n  font-size: 1.4em;\n  margin-top: 0;\n  margin-bottom: .5em;\n}\n#signupButton {\n  margin-left: 1%;\n}\n.container-1 {\n  margin-left: 1%;\n}\n#container-2 {\n  background-color: white;\n  padding: 2em;\n  border-radius: 2em;\n  padding-top: 1.3em;\n}\n@media only screen and (max-width: 1100px) {\n  main {\n    height: 85vh;\n    border-radius: 1em;\n    padding: 0 5%;\n    margin: 0 5vw;\n    background-color: transparent;\n  }\n}\n</style>"],"sourceRoot":"src/components"}]}